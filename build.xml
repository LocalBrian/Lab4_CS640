<project name="TCPend" default="compile">
    <target name="compile">
        <javac includeantruntime="false" srcdir="src/" destdir="src/" debug="on" />
    </target>
	
    <target name="clean">
        <delete>
            <fileset dir="src/" includes="**/*.class"/>
        </delete>
    </target>    
</project>
